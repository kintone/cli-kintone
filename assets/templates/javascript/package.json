{
  "name": "kintone-plugin-template-javascript",
  "version": "0.1.0",
  "scripts": {
    "start": "node scripts/npm-start.js",
    "upload": "cli-kintone plugin upload --input dist/plugin.zip --watch",
    "develop": "npm run build -- --watch",
    "build": "cli-kintone plugin pack --private-key private.ppk --output dist/plugin.zip --input src/manifest.json",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^24.0.0",
    "@kintone/cli": "^1.14.3",
    "npm-run-all": "^4.1.5",
    "eslint": "^9.18.0"
  }
}
