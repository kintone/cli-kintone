  "description": {
    "en": "Description of hello-kintone in English.",
    "ja": "Description of hello-kintone in Japanese",
    "zh": "Description of hello-kintone in Chinese",
    "es": "Description of hello-kintone in Espanol"
  },
  "homepage_url": {
    "en": "https://example.com/en/",
    "ja": "https://example.com/ja/",
    "zh": "https://example.com/zh/",
    "es": "https://example.com/es/"
  },
  "mobile": {
    "js": ["js/mobile.js"],
    "css": ["css/mobile.css"]
  }
