{
  "name": "@kintone/cli-kintone",
  "version": "0.11.2",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "description": "A cli-kintone",
  "bin": {
    "cli-kintone": "cli.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "yarn build --watch",
    "prebuild": "yarn clean",
    "build": "tsc --build --force",
    "postbuild": "yarn pkg",
    "pkg": "pkg . -o bin/cli-kintone -t linux,macos,win",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "lint": "run-p -l lint:*",
    "lint:eslint": "eslint --ext .js,.mjs,.cjs,.ts,.mts,.cts ./",
    "lint:md": "prettier --check ./**/*.md",
    "fix": "run-p fix:*",
    "fix:lint": "run-s 'lint:eslint --fix'",
    "fix:md": "run-s 'lint:md --write'",
    "clean": "rimraf lib bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/cli-kintone.git"
  },
  "files": [
    "lib",
    "cli.js"
  ],
  "keywords": [
    "kintone"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kintone/cli-kintone/issues"
  },
  "homepage": "https://github.com/kintone/cli-kintone/#readme",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@cybozu/eslint-config": "^17.0.2",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@types/yargs": "^17.0.10",
    "babel-jest": "^28.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "jest": "^28.1.2",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^3.1.5",
    "csv-parse": "^4.16.3",
    "iconv-lite": "^0.6.3",
    "yargs": "^17.5.1"
  }
}
