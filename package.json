{
  "name": "@teppeis/kintone-plugin-packer",
  "description": "Package your kintone plugin with pure JavaScript",
  "version": "0.2.1",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=4"
  },
  "main": "src/index.js",
  "bin": {
    "kintone-plugin-packer": "bin/cli.js"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "circleci": "run-s test",
    "css": "cssnano node_modules/normalize.css/normalize.css --safe > docs/dist/normalize.min.css",
    "eslint": "eslint bin src site test",
    "js": "run-s js:browserify js:minify",
    "js:browserify": "browserify site/index.js -i graceful-fs -i glob -i walkdir -i tar-stream -o docs/dist/bundle.js",
    "js:minify": "closure-compiler --js docs/dist/bundle.js --language_out ES6 -O WHITESPACE_ONLY > docs/dist/bundle.min.js",
    "mocha": "mocha --require intelli-espower-loader",
    "site": "run-p js css",
    "test": "run-s eslint mocha"
  },
  "dependencies": {
    "@teppeis/kintone-plugin-manifest-validator": "^0.1.0",
    "debug": "^2.6.3",
    "denodeify": "^1.2.1",
    "meow": "^3.7.0",
    "node-dir": "^0.1.16",
    "node-rsa": "~0.4.2",
    "stream-buffers": "^3.0.1",
    "yazl": "^2.4.2"
  },
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "browserify": "^14.1.0",
    "closure-compiler-cli": "^1.0.1",
    "cssnano-cli": "^1.0.5",
    "eslint": "^3.18.0",
    "eslint-config-teppeis": "^5.3.0",
    "glob": "^7.1.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.2.0",
    "normalize.css": "^6.0.0",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.2",
    "rimraf": "^2.6.1",
    "setimmediate": "^1.0.5",
    "sinon": "^2.1.0",
    "yauzl": "^2.7.0"
  },
  "homepage": "https://github.com/teppeis/kintone-plugin-packer",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/teppeis/kintone-plugin-packer.git"
  },
  "bugs": "https://github.com/teppeis/kintone-plugin-packer/issues",
  "keywords": [
    "kintone"
  ],
  "license": "MIT"
}
