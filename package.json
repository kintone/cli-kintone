{
  "name": "@kintone/plugin-packer",
  "description": "Package your kintone plugin with pure JavaScript",
  "version": "1.0.3",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=6"
  },
  "main": "src/index.js",
  "bin": {
    "kintone-plugin-packer": "bin/cli.js"
  },
  "files": [
    "bin",
    "src",
    "from-manifest.js"
  ],
  "scripts": {
    "circleci": "run-s test",
    "css": "cssnano node_modules/normalize.css/normalize.css --safe > docs/dist/normalize.min.css",
    "eslint": "eslint *.js bin src site test",
    "js": "webpack --mode production",
    "js:dev": "webpack-dev-server --mode development",
    "mocha": "mocha --require intelli-espower-loader",
    "release": "run-s test standard-version",
    "site": "run-p js css",
    "site:dev": "run-p css js:dev",
    "site:test": "mocha site/test --require site/test/setup.js",
    "standard-version": "standard-version",
    "test": "run-s eslint mocha site:test"
  },
  "dependencies": {
    "@kintone/plugin-manifest-validator": "^1.0.2",
    "chokidar": "^2.0.4",
    "debug": "^4.0.1",
    "denodeify": "^1.2.1",
    "meow": "^5.0.0",
    "mkdirp": "^0.5.1",
    "node-rsa": "~1.0.1",
    "stream-buffers": "^3.0.2",
    "yauzl": "^2.10.0",
    "yazl": "^2.4.3"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^2.2.1",
    "adm-zip": "^0.4.11",
    "array-flatten": "^2.1.1",
    "cssnano-cli": "^1.0.5",
    "eslint": "^5.6.1",
    "execa": "^1.0.0",
    "glob": "^7.1.3",
    "husky": "^1.1.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.2.0",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.1",
    "prettier": "1.14.3",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "setimmediate": "^1.0.5",
    "sinon": "^6.3.4",
    "standard-version": "^4.4.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "homepage": "https://github.com/kintone/plugin-packer",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/kintone/plugin-packer.git"
  },
  "bugs": "https://github.com/kintone/plugin-packer/issues",
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run site && git add docs"
    }
  },
  "keywords": [
    "kintone"
  ],
  "license": "MIT"
}
