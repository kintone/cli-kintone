{
  "name": "@teppeis/kintone-plugin-packer",
  "description": "Package your kintone plugin with pure JavaScript",
  "version": "0.1.0",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=4"
  },
  "main": "src/index.js",
  "bin": {
    "kintone-plugin-packer": "bin/cli.js"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "browserify": "browserify site/index.js --ignore graceful-fs -o docs/dist/bundle.js",
    "circleci": "run-s test",
    "cp": "cpr node_modules/normalize.css/normalize.css docs/dist/ -o",
    "eslint": "eslint bin src site test",
    "mocha": "mocha --require intelli-espower-loader",
    "site": "run-p cp browserify",
    "test": "run-s eslint mocha"
  },
  "dependencies": {
    "archiver": "^1.3.0",
    "browserify": "^14.1.0",
    "debug": "^2.6.3",
    "denodeify": "^1.2.1",
    "meow": "^3.7.0",
    "node-rsa": "~0.4.2",
    "recursive-readdir": "^2.1.1",
    "stream-buffers": "^3.0.1"
  },
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "cpr": "^2.0.2",
    "eslint": "^3.18.0",
    "eslint-config-teppeis": "^5.3.0",
    "glob": "^7.1.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.2.0",
    "normalize.css": "^6.0.0",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.2",
    "rimraf": "^2.6.1",
    "setimmediate": "^1.0.5",
    "sinon": "^2.1.0"
  },
  "homepage": "https://github.com/teppeis/kintone-plugin-packer",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/teppeis/kintone-plugin-packer.git"
  },
  "bugs": "https://github.com/teppeis/kintone-plugin-packer/issues",
  "keywords": [
    "kintone"
  ],
  "license": "MIT"
}
