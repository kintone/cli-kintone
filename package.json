{
  "name": "@kintone/cli-kintone",
  "version": "1.10.13",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "description": "A cli-kintone",
  "bin": {
    "cli-kintone": "cli.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "run-s clean 'build:ts --watch'",
    "build": "run-s clean build:ts package",
    "build:ts": "ncc build ./src/cli/main.ts -m",
    "build:artifacts": "run-s build license:analyze license:extract compress",
    "package": "pkg ./dist/index.js --compress Brotli -o bin/cli-kintone -t linux,macos,win",
    "license:analyze": "license-manager analyze -c license-manager.config.js",
    "license:extract": "license-manager extract -c license-manager.config.js",
    "compress": "zx ./scripts/compress-to-zip-file.mjs",
    "typecheck": "tsc --pretty --noEmit",
    "test": "jest",
    "test:ci": "pnpm clean:test && jest --runInBand -c jest.config.ci.js",
    "test:e2e": "cucumber-js",
    "test:e2e:ci": "pnpm clean:test && cucumber-js --format ./cucumber-reporter.js:./allure-results/dummy.txt",
    "lint": "run-p -l lint:*",
    "lint:eslint": "eslint --ext .js,.mjs,.cjs,.ts,.mts,.cts ./ --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint/",
    "lint:prettier": "prettier --check '**/*.{json,md,yml,yaml}' --cache",
    "fix": "run-p fix:*",
    "fix:lint": "run-s 'lint:eslint --fix'",
    "fix:prettier": "run-s 'lint:prettier --write'",
    "clean": "rimraf lib bin artifacts dist",
    "clean:test": "rimraf allure-results allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/cli-kintone.git"
  },
  "files": [
    "lib",
    "dist",
    "cli.js"
  ],
  "keywords": [
    "kintone"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kintone/cli-kintone/issues"
  },
  "homepage": "https://github.com/kintone/cli-kintone/#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@cucumber/cucumber": "^10.4.0",
    "@cybozu/eslint-config": "^23.0.0",
    "@cybozu/license-manager": "^1.2.1",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.29",
    "@types/rollup-plugin-auto-external": "^2.0.5",
    "@types/yargs": "^17.0.32",
    "@vercel/ncc": "^0.38.1",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "zx": "^7.2.3",
    "ajv": "^8.12.0",
    "allure-commandline": "^2.25.0",
    "allure-cucumberjs": "^2.10.0",
    "allure-jest": "^2.10.0"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^5.5.0",
    "chalk": "4",
    "csv-parse": "^4.16.3",
    "csv-stringify": "5.6.5",
    "https-proxy-agent": "^7.0.4",
    "iconv-lite": "^0.6.3",
    "inquirer": "^8.2.6",
    "yargs": "^17.7.2"
  },
  "packageManager": "pnpm@8.15.6"
}
