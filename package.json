{
  "name": "@kintone/cli-kintone",
  "version": "1.2.0",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "description": "A cli-kintone",
  "bin": {
    "cli-kintone": "cli.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "yarn build --watch",
    "prebuild": "yarn clean",
    "build": "tsc --build --force",
    "postbuild": "yarn pkg",
    "build:artifacts": "run-s build licensed compress",
    "pkg": "vite build --force && pkg ./dist/cli/main.js --compress Brotli -o bin/cli-kintone -t linux,macos,win",
    "licensed": "zx ./scripts/generate-license-notice.mjs",
    "compress": "zx ./scripts/compress-to-zip-file.mjs",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "lint": "run-p -l lint:*",
    "lint:eslint": "eslint --ext .js,.mjs,.cjs,.ts,.mts,.cts ./",
    "lint:md": "prettier --check ./**/*.md",
    "fix": "run-p fix:*",
    "fix:lint": "run-s 'lint:eslint --fix'",
    "fix:md": "run-s 'lint:md --write'",
    "clean": "rimraf lib bin artifacts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/cli-kintone.git"
  },
  "files": [
    "lib",
    "cli.js"
  ],
  "keywords": [
    "kintone"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kintone/cli-kintone/issues"
  },
  "homepage": "https://github.com/kintone/cli-kintone/#readme",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@cybozu/eslint-config": "^18.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/yargs": "^17.0.13",
    "babel-jest": "^29.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "jest": "^29.4.1",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.0",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "zx": "^7.1.1",
    "vite": "^4.0.4",
    "glob": "^8.1.0",
    "rollup-plugin-multi-input": "^1.3.3"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^3.3.0",
    "chalk": "4",
    "csv-parse": "^4.16.3",
    "csv-stringify": "5.6.5",
    "https-proxy-agent": "^5.0.1",
    "iconv-lite": "^0.6.3",
    "yargs": "^17.6.2"
  }
}
