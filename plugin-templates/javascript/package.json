{
  "name": "kintone-plugin-template-javascript",
  "version": "0.1.0",
  "scripts": {
    "start": "node scripts/npm-start.js",
    "upload": "cli-kintone plugin upload --input dist/plugin.zip --watch",
    "develop": "npm run build -- --watch",
    "keygen": "cli-kintone plugin keygen --output private.ppk",
    "build": "cli-kintone plugin pack --private-key private.ppk --output dist/plugin.zip --input manifest.json",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^24.0.0",
    "@kintone/cli": "^1.16.1",
    "npm-run-all": "^4.1.5",
    "eslint": "^9.39.1"
  }
}
