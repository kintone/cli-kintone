{
  "name": "kintone-plugin-template-typescript",
  "version": "0.1.0",
  "scripts": {
    "prestart": "npm run build",
    "start": "node scripts/npm-start.js",
    "upload": "cli-kintone plugin upload --input dist/plugin.zip --watch",
    "develop": "npm run build -- --watch",
    "keygen": "cli-kintone plugin keygen --output private.ppk",
    "build": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "dts-gen": "kintone-dts-gen",
    "lint": "eslint src"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^5.5.2",
    "core-js": "^3.40.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@cybozu/eslint-config": "^24.0.0",
    "@kintone/cli": "^1.19.0",
    "@kintone/dts-gen": "^9.0.2",
    "@kintone/webpack-plugin-kintone-plugin": "^8.0.11",
    "ajv": "^8.17.1",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "webpack": "^5.105.2",
    "webpack-cli": "^5.1.4"
  }
}
